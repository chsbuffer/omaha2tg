{
	"compilerOptions": {
		"noEmit": true,
		"module": "esnext",
		"target": "es2021",
		"lib": ["es2021"],
		"strict": true,
		"alwaysStrict": true,
		"preserveConstEnums": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"types": ["@cloudflare/workers-types"],
		"allowSyntheticDefaultImports": true
	},
	"include": ["src"],
	"exclude": ["node_modules", "dist", "test"]
}
